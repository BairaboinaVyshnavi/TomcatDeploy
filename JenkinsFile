node{
  stage('SCM Checkout'){
    git 'https://github.com/BairaboinaVyshnavi/Helloworld.git'
  }
  stage('compile-package'){
    // Get maven home path
    def mvnHome = tool name: 'mavan3', type: 'maven'
    sh "${mvnHome}/bin/mvn package" 
  }
  //stage('Email Notification'){}

  stage('Deploy to Tomcat'){

    sshagent(['tomcat-dev']) {
    sh 'scp -o StrictHostKeyChecking=no target/*.jar ec2-user@172.31.6.50://opt/apache-maven-3.6.3/lib'
}
  }
}
